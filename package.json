{
  "name": "react-ease-picker",
  "version": "0.0.6",
  "main": "dist/index.js",
  "module": "dist/module.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:Apozhidaev/react-ease-picker.git",
  "author": "alexanderpo <alexander.pozhidaev@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "date",
    "range",
    "src"
  ],
  "scripts": {
    "clean": "rm -rf dist/common",
    "build:ts": "tsc && yarn run clean",
    "build:cjs": "esbuild src/index.tsx --format=cjs --bundle --outfile=dist/index.js --external:react*",
    "build:esm": "esbuild src/index.tsx --format=esm --bundle --outfile=dist/module.js --external:react*",
    "build:date:cjs": "esbuild src/date.tsx --format=cjs --bundle --outfile=date/index.js --external:react*",
    "build:date:esm": "esbuild src/date.tsx --format=esm --bundle --outfile=date/module.js --external:react*",
    "build:range:cjs": "esbuild src/range.tsx --format=cjs --bundle --outfile=range/index.js --external:react*",
    "build:range:esm": "esbuild src/range.tsx --format=esm --bundle --outfile=range/module.js --external:react*",
    "build": "yarn run build:cjs && yarn run build:esm && yarn run build:date:cjs && yarn run build:date:esm && yarn run build:range:cjs && yarn run build:range:esm && yarn run build:ts"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "react-easepick": "^0.0.6",
    "react-use-event-hook": "^0.9.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "esbuild": "^0.16.9",
    "react": "^18.2.0",
    "typescript": "^4.9.4"
  }
}
